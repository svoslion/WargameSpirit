<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		<link rel="stylesheet" href="assets/styles.css">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		<header>
			<ul class="menu-list-header">
				<li class="list-element">ACCUEIL</li>
				<li class="list-element">BOUTIQUE</li>
				<img class="logo-header" src="assets/img/logo.png" alt="">
				<li class="list-element">INSCRIPTION</li>
				<li class="list-element">CONNEXION</li>
			</ul>
		</header>
		{% block body %}{% endblock %}
	</body>
</html>
